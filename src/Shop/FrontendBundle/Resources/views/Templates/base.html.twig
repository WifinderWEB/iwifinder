    <![endif]-->

</head>

<body id="page-top" class="index" ng-app="shop">

{% include "ShopFrontendBundle:Templates:_header.html.twig" %}

<!-- Navigation -->
{% block breadcrumbs %}
{#{% include "ShopFrontendBundle:Menu:_breadcrumbs.html.twig" %}#}
{% endblock %}

<!-- Hero Content -->
{% block body %}
{% endblock %}

<!-- Footer -->
{% include('ShopFrontendBundle:Templates:_footer.html.twig') %}

