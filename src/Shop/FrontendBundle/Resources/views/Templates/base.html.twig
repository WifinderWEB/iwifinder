    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
        {% stylesheets  filter='cssrewrite, ?yui_css' output='assetic/css/compiled/frontend.min.css'
        'bundles/shopfrontend/css/page.css'
        'bundles/shopfrontend/css/template.css'
        'bundles/shopfrontend/css/mainpage.css'
        %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}

    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

